<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.MediCAL" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <item name="android:forceDarkAllowed">false</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/verdeClaro</item>
        <item name="windowNoTitle">true</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/verdeClaro</item>
        <item name="android:windowLightStatusBar">true</item>


        <item name="colorAccent">@color/verdeTextos</item>
        <!-- Customize your theme here. -->
    </style>
</resources>